import javax.swing.*;

public class StudentRunner {

    public void run() {

        Student firstStudent1 = new Student("Javohir", "Hoshimov", "220899");

        Student secondStudent2 = new Student("Azlar", "Khasankhojayev", "220263");

        Student thirdstudent3 = new Student("Jamshid", "Rashidov", "221163");

